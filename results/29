<script type="text/plain" cookie-consent="tracking">
    purechatApi.on('chatbox:ready', function () {

        var available = purechatApi.get('chatbox.available');

        if (available) {
            console.log('set chat to available');
            $('.ifchatavailable').show();
            $('.ifchatoffline').hide();
        } else {
            console.log('set chat to unavailable');
            $('.ifchatavailable').hide();
            $('.ifchatoffline').show();
        }

    });

    var chatbox = {
        open: function() {
            purechatApi.set('chatbox.expanded', true);
        }
       };
</script>

