<script>
	jQuery(function ($) {
    $(document).ready(function(){

			const nav = document.querySelector('.main-nav');
			const path = location.pathname.replace(/\/$/, "");
			const link = nav.querySelector(`a[href="${path}"]`);
			if (link) {
				const navCategory = link.closest("[data-navigation-category]");
				if (navCategory) {
					navCategory.classList.add("main-nav__menu-item--active");
				}
			}

			$('.mobile-nav__menu-item').on('click', function () {
				if ( $(this).siblings('.mobile-nav__menu-item--open') ) {
					$(this).siblings('.mobile-nav__menu-item--open').removeClass('mobile-nav__menu-item--open');
				}
				$(this).toggleClass('mobile-nav__menu-item--open');
			})
			

			$('.mobile-nav__dropdown-item').on('click', function (e) {
				e.preventDefault();
				const currentUrl = location.pathname.replace(/\/$/, "");
				const targetUrl = $(this).attr('href');
				const targetPathname = targetUrl.split('#');
				if (targetPathname[0].replace(/\/$/, "") === currentUrl) {
					$('.main-nav__mobile-menu.main-nav__mobile-menu--open').removeClass('main-nav__mobile-menu--open');
					$('[data-mobile-product-menu-control]').removeClass('main-nav__burger--open');
					location.href = targetUrl;
				} else {
					location.href = targetUrl;
				}
			})
			
			const burgerMenu = document.querySelector("[data-mobile-product-menu-control]");
			// Burger menu does not exist on PPC pages
			if (burgerMenu) burgerMenu.addEventListener('click', function () {
				burgerMenu.classList.toggle('main-nav__burger--open');
				document.querySelector("[data-mobile-product-menu]").classList.toggle("main-nav__mobile-menu--open");
			})
			
			const loginMenu = document.querySelector("[data-mobile-login-menu-control]");
			loginMenu.addEventListener('click', function () {
				document.querySelector("[data-mobile-login-menu]").classList.toggle("main-nav__mobile-menu--open");
			})

			const closeMenu = document.querySelector("[data-mobile-login-menu-close]");
			closeMenu.addEventListener('click', function () {
				closeMenu.parentElement.classList.remove("main-nav__mobile-menu--open");
			})

			if (document.location.href.indexOf("/dev-products") > -1 || document.location.href.indexOf("/be-virtually-unstoppable") > -1) {
				$("[data-navigation-category='Domains'],[data-navigation-category='Websites'],[data-navigation-category='Email'],[data-navigation-category='Online Marketing']").hide();
			}
		});
	});



</script> 


