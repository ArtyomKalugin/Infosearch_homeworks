<script src="https://fasthosts-us-r.widget.custhelp.com/euf/rightnow/RightNow.Client.js" defer></script>
<script src="https://cdn.jsdelivr.net/npm/js-cookie@rc/dist/js.cookie.min.js" defer></script>
<script>
  jQuery(function ($) {

    $(document).ready(function () {
      $('.livechat-toggle').on('click', function () {
        $('.livechat-bubble').toggleClass('is-opened');
        $('.livechat-popup').toggleClass('mod-visible');
        if (!$.cookie('livechat-closed')) {
          $.cookie('livechat-closed', '1', { expires: 7, path: '/', domain: 'fasthosts.co.uk' });
        }
      });

      function onChatOffered() {
        if (!$.cookie('livechat-closed')) {
          setTimeout(function () { $('.livechat-bubble').addClass('is-opened').siblings('.livechat-popup').addClass('mod-visible'); }, 10000);
        }

        // check if livechat button exists and show live chat if it does
        if ($(window).width() > 470) {
          if ($('.yui-skin-sam .yui-panel .yui-button button').length) {
            $('.livechat').removeClass('mod-hidden');
          }
        };
      }
      // hide chat if the screen is resized
      $( window ).resize(function() {
        if (this.innerHeight > 646 && this.innerWidth > 470 ) {
          if ($('.yui-skin-sam .yui-panel .yui-button button').length) {
            $('.livechat').removeClass('mod-hidden');
          }
        } else {
          if ($('.yui-skin-sam .yui-panel .yui-button button').length) {
            $('.livechat').addClass('mod-hidden');
          }
        }
      })

      if (typeof RightNow !== "undefined") {
        RightNow.Client.Controller.addComponent({
          chat_login_page: "/app/chat/chat_landing",
          instance_id: "spac_0",
          min_agents_avail: 1,
          seconds: 1,
          div_id: "chatLink",
          module: "ProactiveChat",
          type: 2,
          label_accept_button: "Chat now",
          // label_reject_button: "No Thanks",
          label_reject_button: "",
          label_title: "",
          logo_image: "",
          // avatar_image: "http://help.fasthosts.com/euf/assets/images_fasthosts/chatnow.png",
          avatar_image: "",
          label_avatar_image: "",
          // label_dialog_header: "Do you need some help?",
          label_dialog_header: "",
          label_refuse_checkbox: "",
          label_question: ""
        },
        "https://fasthosts-us-r.widget.custhelp.com/ci/ws/get");
        //Widget loaded callback
        RightNow.Client.Event.evt_widgetLoaded.subscribe(function (event_name, data) {
          if (data[0].id == "spac_0") {
            //Reset prototype
            spac_0.prototype = {};
            //Handle Chat Offered
            spac_0.prototype.chatOffered = spac_0.chatOffered;
            spac_0.chatOffered = function () {
              onChatOffered();
              // console.log("Chat Offered Handled");
              spac_0.prototype.chatOffered.call(this);
              RightNow.Client.Util.setCookie("noChat", '', -10, '/', '', '');
            };
          }
        });
      }
    });
  });
</script>
  

  


