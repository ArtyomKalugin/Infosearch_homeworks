    var OAO = OAO || {}; // OAO refers to the global OAO variable.
    OAO.q = OAO.q || {};
    OAO.q.c = OAO.q.c || [];
    OAO.q.c.push(['setPageName', '/domain-names/co-uk-domains']);

    OAO.q.ias = OAO.q.ias || [];
    OAO.q.ias.push(["addVisitorData", "signup-host-name", "https://admin.fasthosts.co.uk"]);

    OAO.q.ias.push(["addVisitorData", "host-name", "https://www.fasthosts.co.uk"]);
    OAO.q.ias.push(["addVisitorData", "admin-site", "https://admin.fasthosts.co.uk"]);
    OAO.q.ias.push(["addVisitorData", "marketing-site", "https://www.fasthosts.co.uk"]);
    OAO.q.ias.push(["addVisitorData", "shop-site", "https://www.fasthosts.co.uk"]);
    OAO.q.ias.push(["addVisitorData", "assets-site", "https://static.fasthosts.co.uk"]);

    const queryString = window.location.search;
    const urlParams = new URLSearchParams(queryString);
    const keys = urlParams.keys(); 
    for (const key of keys) {
        if (key.startsWith('ias.')) {
            OAO.q.ias.push(['addVisitorData', key.substring(4), urlParams.get(key)]);
        }
    }
</script>

