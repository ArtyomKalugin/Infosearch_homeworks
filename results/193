<div class="fh-tabs-horizontal ">
  <div role="tabpanel" class="fh-tabs-horizontal__tabpanel">
    <div class="fh-tabs-horizontal__tabs-container">
      
        <div
          data-tab-image="/img/new-images/feature/co-uk-tea.svg"
          data-tab-icon="/img/new-images/icons/icon-tea-circle-primary.svg"
          data-tab-index="0"
          data-tab-parent-class=""
          class="
            fh-tabs-horizontal__tab
            fh-tabs-horizontal__tab--line-colour-primary
            fh-tabs-horizontal__tab--inactive
          "
          style="
            --icon-open: url(/img/new-images/icons/icon-tea-circle-primary.svg);
