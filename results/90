</div>
</div><div class="et_pb_column et_pb_column_1_2 et_pb_column_2  et_pb_css_mix_blend_mode_passthrough et-last-child">
<div class="et_pb_module et_pb_code et_pb_code_0">
<div class="et_pb_code_inner">
<div class="fitvidsignore"> <style type="text/css">
			div.updraft-fluid-width-video-wrapper iframe, div.updraft-fluid-width-video-wrapper video {
				width: 100% !important;
				max-width: 100% !important;
			}
			div.updraft-fluid-width-video-wrapper iframe {
				position: absolute !important;
				top: 0 !important;
				left: 0 !important;
				height: 100% !important;
			}
			div.updraft-fluid-width-video-wrapper button:hover {
				background-color: rgb(223, 105, 38) !important;
			}
		</style>
<div class="updraft-fluid-width-video-wrapper" style="width: 100%; position: relative; padding: 0;"></div>
<script type="text/javascript">
			window.wp = window.wp || {};
			(function($) {
				'use strict';
								var updraft_video = wp.updraft_video = wp.updraft_video || {},
					unique_id = Math.random().toString(16).slice(2), // set a unqiue ID to prevent loading JS resource from browser's cache and making sure the onload or onerror event will always get triggered
					can_access = updraft_video.can_access = updraft_video.can_access || {
						vimeo: window.Vimeo = window.Vimeo || false, // get Vimeo player API object if it's already loaded by other plugins
						youtube: window.YT = window.YT || false // get Youtube player API object if it's already loaded by other plugins
					},
					third_party_hostings = {
						vimeo: 'https://player.vimeo.com/api/player.js?'+unique_id,
						youtube: 'https://youtube.com/player_api?'+unique_id
					},
					init_event_handlers = wp.updraft_video.init_event_handlers = wp.updraft_video.init_event_handlers || function(wrapper) {
						if ($('iframe', wrapper).length) $(wrapper).css('padding-bottom', '56.25%'); // aspect-ratio 16:9
						$('video', wrapper).on('play pause', function(e) {
							$(this).trigger('mouseenter');
						});
						$('video', wrapper).on('canplay', function(e) {
							$(this).off('canplay');
							$(wrapper).on('mouseleave', function(e) {
								if ('mouseleave' === e.type) {
									$('button', this).find('svg').hide();
									$('button', this).hide();
								}
							});
							$(this).on('mouseenter', function(e) {
								if ('mouseenter' === e.type) {
									$('button', wrapper).find('svg').hide();
									$('button', wrapper).show();
									if (this.ended && this.currentTime === this.duration) {
										$('svg.updraft-video-replay-icon', $('button', wrapper)).show();
									} else if (this.paused && ((this.currentTime > 0 && this.currentTime < this.duration) || this.readyState > 2)) {
										$('svg.updraft-video-play-icon', $('button', wrapper)).show();
									} else if ((!this.paused && !this.ended) || this.currentTime > 0) {
										$('svg.updraft-video-pause-icon', $('button', wrapper)).show();
									}
								}
							});
							$('button', wrapper).on('click', function(e) {
								var vid = $('video', wrapper);
								if (vid[0].paused || vid[0].ended) {
									vid[0].play();
								} else {
									vid[0].pause();
								}
							});
							$(this).trigger('mouseenter');
						});
					},
					iframe_template = wp.updraft_video.iframe_template = wp.updraft_video.iframe_template || '\
						<iframe \
							id="" \
							style="background-color: #0000;" \
							width="" \
							height="" \
							frameborder="0" \
							allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture; " \
							webkitallowfullscreen mozallowfullscreen allowfullscreen \
						> \
						</iframe> \
					',
					video_template = wp.updraft_video.video_template = wp.updraft_video.video_template || '\
						<div> \
							<button class="updraft-video-button-active" label="play" style="width: 6.5em; height: 4em; position: absolute; left: 50%; top: 50%; cursor: pointer; background: rgba(30,30,30,.9); border: 0; padding: 0; border-radius: .5em; margin-left: -3.25em; margin-top: -2em; z-index: 9999; display: none;"> \
								<div style="padding: .2em 0 0 .2em;"> \
									<svg class="updraft-video-play-icon" viewBox="0 0 20 20" style="width: 2em; height: 2em; display: none;" preserveAspectRatio="xMidYMid" focusable="false" aria-hidden="true"> \
										<polygon style="fill: #fff; transition: fill 40ms" points="1,0 20,10 1,20"></polygon> \
									</svg> \
									<svg class="updraft-video-pause-icon" viewBox="0 0 20 20" style="width: 2em; height: 2em; display: none;" preserveAspectRatio="xMidYMid" focusable="false" aria-hidden="true"> \
										<rect style="fill: #fff; transition: fill 40ms" width="5" height="20" x="0" y="0"></rect> \
										<rect style="fill: #fff; transition: fill 40ms" width="5" height="20" x="12" y="0"></rect> \
									</svg> \
									<svg class="updraft-video-replay-icon" viewBox="-387 605 16 16" style="width: 2em; height: 2em; display: none;" preserveAspectRatio="xMidYMid" focusable="false" aria-hidden="true"> \
									<path style="fill: #fff; transition: fill 40ms" d="M-387 606v4c0 .6.4 1 1 1h4c.6 0 1-.4 1-1s-.4-1-1-1h-1.5c1.1-1.2 2.7-2 4.5-2 3.3 0 6 2.7 6 6s-2.7 6-6 6c-2.3 0-4.4-1.3-5.4-3.4-.2-.5-.8-.7-1.3-.5-.5.2-.7.8-.5 1.3 1.3 2.8 4.2 4.6 7.2 4.6 4.4 0 8-3.6 8-8s-3.6-8-8-8c-2.3 0-4.5 1-6 2.7V606c0-.6-.4-1-1-1s-1 .4-1 1z"></path> \
									</svg> \
								</div> \
							</button> \
							<video preload="metadata"> \
								<source type="video/mp4" /> \
								<source type="video/webm" /> \
								<source type="video/ogg" /> \
								Your browser doesn\'t support HTML5 video \
							</video> \
						</div> \
					',
					video_unavailable_template = wp.updraft_video.video_unavailable_template = wp.updraft_video.video_unavailable_template || '\
						<p> \
							<strong>All video resources (Vimeo, Youtube, Self-hosted) are not reachable at the moment.</strong>\
						</p> \
					';
				
				for (var key in third_party_hostings) {
					(function(doc, tag, src, key, elt, fst) {
						elt = doc.createElement(tag), fst=doc.getElementsByTagName(tag)[0];
						elt.async = 1;
						elt.src = src;
						elt.onload = function() {
							window[key] = can_access[key]; // restore Vimeo or YT object to its previous reference, one that is loaded by other plugins (if any)
							can_access[key] = updraft_video.can_access[key] = true;
						};
						elt.onerror = function() {
							window[key] = can_access[key]; // restore Vimeo or YT object to its previous reference, one that is loaded by other plugins (if any)
							can_access[key] = updraft_video.can_access[key] = false;
						}
						fst.parentNode.insertBefore(elt,fst);
					})(document, 'script', third_party_hostings[key], key);
				}
								var wrapper = $('div.updraft-fluid-width-video-wrapper').last(),
					can_access_self_hosted_file = true;
				$(window).on('load', function() {
					var video = $(wp.updraft_video.iframe_template);
															video.prop('id', 'heading-video');
					video.prop('allow', video.prop('allow') + 'autoplay; ');
					if (true === wp.updraft_video.can_access.vimeo && '154870690'.length > 1) {
						video.prop('src', 'https://player.vimeo.com/video/154870690?color=df6926&title=0&byline=0&portrait=0');
					} else if (true === wp.updraft_video.can_access.youtube && '0'.length > 1) {
						video.prop('src', 'https://www.youtube.com/embed/0?rel=0&showinfo=0&autoplay=false&loop=false');
					} else if (can_access_self_hosted_file) {
						video = $(wp.updraft_video.video_template);
						$('video source[type="video/mp4"]', video).prop('src', 'https://updraftplus.com/wp-content/uploads/2022/04/updraftplus.mp4');
						$('video source[type="video/webm"]', video).prop('src', 'https://updraftplus.com/wp-content/uploads/2022/05/updraftplus.webm');
						$('video source[type="video/ogg"]', video).prop('src', 'https://updraftplus.com/wp-content/uploads/2022/05/updraftplus.ogv');
																													} else {
						// display a background image/ transparent background color, which is already done by default
						video = $(wp.updraft_video.video_unavailable_template);
					}
					video.appendTo(wrapper);
					wp.updraft_video.init_event_handlers(wrapper);
				});
			})(jQuery);
		</script>
</div></div>
</div>
</div>
