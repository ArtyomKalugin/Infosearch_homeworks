<script>
    //add in custom animated icons into the search for type-a-head loading
    const addSearchLoaderIcons = () => {

        const iconContainer = document.querySelector('.is-loader-image');
        iconContainer.style.backgroundImage = ''; //removes the plugin gif
        
        //build elements & add class for the jelly bean animation
        const coukIconElement = document.createElement('i')
        coukIconElement.classList.add('badge--couk');

        const meukIconElement = document.createElement('i')
        meukIconElement.classList.add('badge--meuk');

        const orgukIconElement = document.createElement('i')
        orgukIconElement.classList.add('badge--orguk');

        const ukIconElement = document.createElement('i')
        ukIconElement.classList.add('badge--uk');

        //add to the DOM
        iconContainer.append(coukIconElement, meukIconElement, orgukIconElement, ukIconElement)
    }

    const setSearchTypeFilterQuery = () => {

        const form = document.querySelector('#faq-search-form-wrapper form');
        const hiddenSearchInput = document.createElement('input')
        hiddenSearchInput.type = 'hidden';
        hiddenSearchInput.name = 'search_type';
        hiddenSearchInput.value = 'faq-help-centre';
        form.appendChild(hiddenSearchInput)

    }

    //Init on load
    window.addEventListener('load', function () {

        addSearchLoaderIcons();
        setSearchTypeFilterQuery();

    }, false);

</script>




